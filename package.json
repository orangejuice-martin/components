{
  "name": "starter",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "clean": "node scripts/clean.js",
    "copy": "node scripts/copy.js",
    "start": "npm run clean && npm run copy && webpack --mode=development --watch",
