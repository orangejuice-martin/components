    "@types/flubber": "^0.4.0",
    "@types/mithril": "2.0.6",
    "autoprefixer": "^10.4.21",
    "css-loader": "^7.1.2",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^9.1.0",
    "mini-css-extract-plugin": "^2.9.2",
    "null-loader": "^4.0.1",
    "postcss-loader": "^8.1.1",
    "sass": "^1.89.2",
    "sass-loader": "^16.0.5",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.2",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.99.9",
